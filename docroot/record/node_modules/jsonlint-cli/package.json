    "test": "parallelshell \"eslint index.js\" \"node index.js **/*.json *.json\" ",
    "version": "npm run changelog && git add . && echo \"$(conventional-changelog -p angular)\" > .git/COMMITMSG.tmp"
  },
  "version": "1.0.1"
